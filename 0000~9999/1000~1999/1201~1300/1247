import sys
input = sys.stdin.readline

for _ in range(3):
    n = int(input().rstrip())
    
    sum = 0
    for _ in range(n):
        number = int(input().rstrip())
        sum += number
        
    if sum == 0:
        print(0)
    elif sum > 0:
        print('+')
    else:
        print('-')