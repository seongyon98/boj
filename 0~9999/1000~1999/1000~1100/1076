res_value = {"black":0, "brown":1, "red":2, "orange":3, "yellow":4, "green":5, "blue":6, "violet":7, "grey":8, "white":9}
res_multi = {"black":1, "brown":10, "red":100, "orange":1000, "yellow":10000, "green":100000, "blue":1000000, "violet":10000000, "grey":100000000, "white":1000000000}

f = input()
s = input()
m = input()

first_value = res_value[f]
second_value = res_value[s]
multi = res_multi[m]

result = ((first_value*10) + second_value) * multi

print(result)